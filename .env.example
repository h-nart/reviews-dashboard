# Hostaway API Configuration
# client_id = HOSTAWAY_ACCOUNT_ID, client_secret = HOSTAWAY_API_KEY
HOSTAWAY_ACCOUNT_ID=your_hostaway_account_id_here
HOSTAWAY_API_KEY=your_hostaway_api_key_here
HOSTAWAY_API_URL=https://api.hostaway.com/v1

# Server API URL: use '/' on Vercel
REACT_APP_API_URL=/

# Set to 'true' to use mock data instead of real API calls
USE_MOCK_DATA=true

# Server Configuration
PORT=3001
NODE_ENV=development
